{% do form.prepare() %}
{{ form().openTag(form)|raw }}

{% set videoFormElement = form.get('general_entity_country_video') %}

{{ lbs5formelement(videoFormElement.get('video')) }}
{{ lbs5formelement(videoFormElement.get('title')) }}
{{ lbs5formelement(videoFormElement.get('type')) }}
{{ lbs5formelement(videoFormElement.get('description')) }}

<div class="row mb-3">
    <div class="offset-sm-3 col-sm-9">
        {{ formelement(form.get('csrf')) }}
        {{ lbs5formelement(form.get('submit')) }}
        {{ lbs5formelement(form.get('cancel')) }}
        {% if form.has('delete') %}
            {{ lbs5formelement(form.get('delete')) }}
        {% endif %}
    </div>
</div>

{{ form().closeTag()|raw }}