<h3>{{ translate("txt-country-information") }}</h3>
<div role="tabpanel">
    <ul class="nav nav-tabs" role="tablist">
        <li role="presentation" class="active" aria-controls="projects" role="tab" data-toggle="tab">
            <a href="#projects">{{ translate("txt-projects") }} ({{ projects|length }})</a>
        </li>
        <li role="presentation" aria-controls="partners" role="tab" data-toggle="tab">
            <a href="#partners">{{ translate("txt-partners") }} ({{ organisations|length }})</a>
        </li>
         <li role="presentation" aria-controls="members" role="tab" data-toggle="tab">
            <a href="#members">{{ translate("txt-members") }} ({{ members|length }})</a>
        </li>
    </ul>

    <div class="tab-content">
        <div role="tabpanel" class="tab-pane active" id="projects">
            <div>
                <table class="table table-condensed table-striped table-hover">
                     <thead>
                        <tr>
                            <th class="col-xs-8">{{ translate("txt-project") }}</th>
                            <th class="col-xs-4">{{ translate("txt-call") }}</th>
                        </tr>
                    </thead>
                    {% for projectService in projects %}
                     <tr>
                        <td>{{ projectLink(projectService, 'view', 'name-without-number') }}</td>
                        <td>{{ projectService.project.call }}</td>
                     </tr>
                    {% endfor %}
                </table>
            </div>
        </div>
        <div role="tabpanel" class="tab-pane" id="partners">
            <div>
                <table class="table table-condensed table-striped table-hover">
                     <thead>
                        <tr>
                            <th class="col-xs-7">{{ translate("txt-organisation") }}</th>
                            <th class="col-xs-5">{{ translate("txt-type") }}</th>
                        </tr>
                    </thead>
                    {% for organisation in organisations %}
                        {% set organisationService = organisationServiceProxy(organisation) %}
                     <tr>
                         <td>{{ organisationLink(organisationService, 'view', 'name') }}</td>
                         <td>{{ organisation.type }}</td>
                     </tr>
                    {% endfor %}
                </table>
            </div>
        </div>
        <div role="tabpanel" class="tab-pane" id="members">
            <div>
                <table class="table table-condensed table-striped table-hover">
                     <thead>
                        <tr>
                            <th class="col-xs-9">{{ translate("txt-member") }}</th>
                            <th class="col-xs-3">{{ translate("txt-type") }}</th>
                        </tr>
                    </thead>
                    {% for organisation in members %}
                        {% set organisationService = organisationServiceProxy(organisation) %}
                     <tr>
                         <td>{{ organisationLink(organisationService,'view','name') }}</td>
                         <td>
                             {% if organisation.member %}
                                {{ organisation.member.type }}
                             {% else %}
                                {% for cluster in organisation.getClusterMember() %}
                                    {{ cluster.organisation.member.type }}
                                {% endfor %}
                             {% endif %}
                         </td>
                     </tr>
                    {% endfor %}
                </table>
            </div>
        </div>
  </div>
</div>
