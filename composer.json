{
  "name": "iteaoffice/general",
  "type": "library",
  "license": "proprietary",
  "description": "Code to manage general elements",
  "keywords": [
    "general"
  ],
  "homepage": "https://itea3.org",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.itea3.eu"
    }
  ],
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.2",
    "zendframework/zend-mvc": "^3.0.1",
    "zendframework/zend-permissions-acl": "2.7.*",
    "doctrine/common": "2.9.*",
    "mailjet/mailjet-apiv3-php": "1.2.*"
  },
  "require-dev": {
    "iteaoffice/testing": "dev-master",
    "iteaoffice/affiliation": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "General\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GeneralTest\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev",
      "dev-develop": "3.1.x-dev"
    }
  }
}
