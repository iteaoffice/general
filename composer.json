{
  "name": "iteaoffice/general",
  "type": "library",
  "description": "General shared code for ITEA",
  "keywords": [
    "general"
  ],
  "license": "proprietary",
  "homepage": "https://itea3.org",
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">5.5",
    "zendframework/zendframework": "~2.5",
    "doctrine/doctrine-orm-module": "0.9.*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "4.2.*",
    "pdepend/pdepend": "2.0.*",
    "phpmd/phpmd": "2.0.*",
    "sebastian/phpcpd": "*",
    "squizlabs/php_codesniffer": "2.*",
    "fabpot/php-cs-fixer": "0.5.7.",
    "jakub-onderka/php-parallel-lint": "0.*",
    "jakub-onderka/php-console-highlighter": "0.*"
  },
  "autoload": {
    "psr-4": {
      "General\\": "src/General/",
      "GeneralTest\\": "tests/GeneralTest"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev",
      "dev-develop": "1.2-dev"
    }
  }
}
