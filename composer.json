{
    "name": "debranova/general",
    "type": "library",
    "description": "General shared code for Debranova",
    "keywords": ["general"],
    "license": "proprietary",
    "homepage": "http://debranova.org",
    "authors": [
        {
            "name": "Johan van der Heide",
            "email": "johan.van.der.heide@itea2.org"
        },
        {
            "name": "Andre Hebben",
            "email": "andre.hebben@artemis-ia.eu"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework": "2.3.*@dev",
        "doctrine/doctrine-orm-module": "0.9.*@dev",
        "gedmo/doctrine-extensions": "2.3.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*@dev",
        "squizlabs/php_codesniffer": "2.0.*@dev",
        "fabpot/php-cs-fixer": "0.5.*@dev",
        "doctrine/data-fixtures": "2.0.*@dev"
    },
    "autoload": {
        "psr-0": {
            "General\\": "src/",
            "GeneralTest\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.2-dev"
        }
    }
}
