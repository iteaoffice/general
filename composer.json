{
  "name": "iteaoffice/general",
  "type": "library",
  "description": "General shared code for ITEA",
  "keywords": [
    "general"
  ],
  "license": "proprietary",
  "homepage": "https://itea3.org",
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6",
    "zendframework/zendframework": "~2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "pdepend/pdepend": "@stable",
    "phpmd/phpmd": "@stable",
    "sebastian/phpcpd": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "fabpot/php-cs-fixer": "@stable",
    "jakub-onderka/php-parallel-lint": "@stable",
    "jakub-onderka/php-console-highlighter": "@stable"
  },
  "autoload": {
    "psr-4": {
      "General\\": "src/General/",
      "GeneralTest\\": "tests/GeneralTest"
    }
  },
  "config": {
    "secure-http": false
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev",
      "dev-develop": "1.2-dev"
    }
  }
}
