{
  "name": "iteaoffice/general",
  "type": "library",
  "description": "General shared code for ITEA",
  "keywords": [
    "general"
  ],
  "license": "proprietary",
  "homepage": "https://itea3.org",
  "authors": [
    {
      "name": "Johan van der Heide",
      "email": "johan.van.der.heide@itea3.org"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.1",
    "zendframework/zendframework": "~3.0"
  },
  "require-dev": {
    "iteaoffice/testing": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "General\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GeneralTest\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev",
      "dev-develop": "2.1-dev"
    }
  }
}
